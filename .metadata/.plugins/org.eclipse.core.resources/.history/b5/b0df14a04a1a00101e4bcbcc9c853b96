package com.novelsbr.backend.enums;

public enum GenderType {

	ADVENTURE(1, "Aventura"),
	DYSTOPIA(2, "Distopia"),
	FANTASY(3, "Fantasia"),
	HISTORICAL(4, "Historical"),
	HORROR(5, "Horror"),
	LGBTQ(6, "LGBTQ"),
	MAGICAL(7, "Magical"),
	MYSTERY(8, "Mistery"),
	ADULT(9, "New Adult"),
	ROMANCE(10, "Romance"),
	SCIENCE_FICTION(11, "Science Fiction"),
	THRILLER(12, "Thriller"),
	WESTERN(13, "Western"),
	SUSPENSEFUL(14, "Suspenseful"),
	ACTION(15, "Action"),
	MEDIEVAL(16, "Medieval");
	
	private Integer cod;
	
	private String type;
	
	private GenderType(Integer cod, String type) {
		this.cod = cod;
		this.type = type;
	}

	public Integer getCod() {
		return cod;
	}

	public String getType() {
		return type;
	}
}
